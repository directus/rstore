{
  "name": "@rstore/nuxt",
  "type": "module",
  "version": "0.8.2",
  "description": "Reactive Store for Nuxt",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Akryum/rstore.git",
    "type": "git",
    "directory": "packages/nuxt"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxi prepare playground && nuxt-module-build build && npm run client:build",
    "dev": "pnpm run build",
    "dev:prepare": "nuxt prepare && pnpm run client:prepare",
    "dev:playground": "nuxt dev ../playground",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "client:prepare": "nuxi prepare client",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.2.1",
    "@nuxt/kit": "^4.0.0",
    "@rstore/shared": "workspace:^",
    "@rstore/vue": "workspace:^",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.28",
    "@nuxt/devtools": "^2.0.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "catalog:",
    "@nuxt/test-utils": "^3.15.4",
    "@nuxt/ui": "catalog:",
    "@types/node": "catalog:",
    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "^14.0.0",
    "nuxt": "catalog:",
    "shiki": "^3.1.0",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  }
}
